{
    "name": "vocola2",
    "displayName": "vocola2",
    "description": "Vocola voice command language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "vocola",
            "aliases": ["vocola", "vocola"],
            "extensions": [".vch",".vcl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "vocola",
            "scopeName": "source.vocola",
            "path": "./syntaxes/vocola.tmLanguage.json"
        }]
    }
}